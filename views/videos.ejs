<%- include('partials/header') %>
<div class="max-w-6xl mx-auto p-8">
    <h1 class="text-3xl font-black mb-6">Video Library (Tier <%= user.membershipTier %>)</h1>
    <div class="grid grid-cols-1 md:grid-cols-2 gap-8">
        <% videos.forEach(vid => { %>
            <div class="bg-white rounded-3xl overflow-hidden border shadow-sm">
                <div class="aspect-video bg-black flex items-center justify-center text-white">
                    <p><%= vid.title %></p>
                </div>
                <div class="p-4 font-bold text-center"><%= vid.title %></div>
            </div>
        <% }) %>
    </div>
    <a href="/dashboard" class="mt-8 inline-block text-gray-500">‚Üê Kembali ke Dashboard</a>
</div>